#%RAML 1.0 DataType
type: array
description: List of cabins' details
items:
  additionalProperties: false
  properties:
    id:
      type: string
      description: The cabin identifier
      required: true
      example: "123"
    price:
      type: !include price-details-optional.raml
      required: false
    couponCode:
      type: !include coupon-details.raml
      required: false
    promotion:
      type: !include promotion-id.raml
      required: false