#%RAML 1.0 DataType
type: array
description: List of passengers' details
items:
  type: !include passenger-id.raml
  additionalProperties: false
  properties:
    price:
      type: !include price-details-optional.raml
      required: false
    couponCode:
      type: !include coupon-details.raml
      required: false
    promotion:
      type: !include promotion-id.raml
      required: false